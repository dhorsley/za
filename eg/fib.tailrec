#!/usr/bin/za

define fib(n)
    return fib_tr(n,as_bigi(0),as_bigi(1))
end

define fib_tr(n, a, b)
    on n==0 do return a
    on n==1 do return b
    return fib_tr(n-1,b,a+b)
end

input x param 1 is "start of range"
input y param 2 is "end of range"

on not (x is int and y is int) do exit 1,"parameters must be integers."

for i=x to y
    println i," ",fib(i)
ef

