#/usr/bin/za

# long loop test - checks base FOR speed.

module "./modules/timing.mod"

max_count=100000000
input max_count optarg 1
# on kind(mc)=="int" do max_count=int(mc)

ts=epoch_nano_time()
for x=1 to max_count
endfor
println "x: ",kind(x)," ",x
te=epoch_nano_time()
assert x==max_count

#
# END
#

timing.report(ts,te,max_count)

