#!/usr/bin/za -f

total=10
user=20
nice=30
sys=40
idle=50
iowait=40
irq=30
softirq=20
steal=10
guest=0
guest_nice=50

init prev

name="cpu0"

# local
i="sys"

# indirect j="sys"
j="i"

println "ind. i = {i}"
println "ind. j = {j}"

# double indirect j=sys=40
prev["{name}_{i}"]=int({i})
prev["{name}_{i}"]=int({{j}})

println "stored type : ",kind(prev["{name}_{{j}}"])
println "Name : {name}"
println "Append(i) : {i}"
println "prev values list :\n",prev

