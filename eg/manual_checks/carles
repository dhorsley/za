#!/usr/bin/za
 
var il1 int
var il2 int
var il3 int
var ic int

println "Performance Test by Carles Mateo blog.carlesmateo.com"    
println "Za Version: "

print "Starting...", epoch_time()
    
for il1 = 0 To 9
    for il2 = 0 To 31999
        for il3 = 0 To 31999
            ic=ic+1
            on ic > 50 do ic=0
        endfor
    endfor
endfor

println ic
Print "End ", epoch_time() 


