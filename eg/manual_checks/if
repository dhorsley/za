
debug 0

c1=false; c2=false
input ic1 optarg 1
input ic2 optarg 2
on kind(ic1)=="bool" do c1=bool(ic1)
on kind(ic2)=="bool" do c2=bool(ic2)

if c1
    if true 
        if c2
            println "smeagol!"
        else
            println "gollum!"
        endif
    else
        println "gandalf."
    endif
    println "monty"
endif

println "Complete."



