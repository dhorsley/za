
debug 0

c1=false; c2=false
input ic1 optarg 1
input ic2 optarg 2
on ic1 ~i "^(true|false)$" do c1=as_bool(ic1)
on ic2 ~i "^(true|false)$" do c2=as_bool(ic2)

if c1
    if true 
        if c2
            println "smeagol!"
        else
            println "gollum!"
        endif
    else
        # should never appear:
        println "gandalf."
    endif
    # should always appear if arg1 true
    println "monty"
endif

println "Complete."



