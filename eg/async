#!/usr/bin/za

define q(val)
    delay=rand(4)
    println val
    pause delay*1000
    return string(val)
end

# MAIN

coproc(false)

println "Spinning async tasks"

for e = 1 to 400
    async handles q(e)
endfor

while true

    println "[#2]Waiting...[#-]"
    res=await(handles,true)
    println "await() complete."
#    println "len res=",res.len

    on len(res)==0 do break

    foreach a in res
        println format("{key_a} -> [%v] %v",kind(a),a)
    endfor

    println
    pause 50

endwhile
