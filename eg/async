#!/usr/bin/za

define q(val)
    delay=rand(4)
    # println val
    pause delay*100
    return as_string(val)
end

# MAIN

coproc(false)

println "Spinning async tasks"

for e = 0 to 99
    async handles q(e)
endfor

# while true

    println "[#2]Waiting...[#-]"
    res=await("handles",true)
    println "await() complete."

    on len(res)==0 do break

    foreach a in res
        println format("{key_a} -> [%v] %v",kind(a),a)
    endfor

    println
    pause 50

# endwhile
