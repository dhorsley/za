#!/usr/bin/za

define q(val)
    delay=rand(4)
    println val
    pause delay*100
    return as_string(val)
end

# MAIN

coproc(false)

println "Spinning async tasks"

for e = 0 to 399
    async handles q(e)
ef

while true

    println "[#2]Waiting...[#-]"
    res=await(ref handles,true)
    println "await() complete."

    on len(res)==0 do break

    foreach a in res
        println format("{key_a} -> [%v] %v",kind(a),a)
    ef

    println
    pause 50

ew
