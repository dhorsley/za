#!/usr/bin/za


define z()
    return
end

define a()
    return 10+0
end

define b()
    return 5+pi()
end

define c()
    return 10,(20),("thirty"+" twelve"),20/2*3+(1+2)
end

define d()
    return a(),b(),a()+b(),(c())
end

println "z -> ",z()
println "a -> ",a()
println "b -> ",b()
c1,c2,c3,c4=c()
println "c -> {c1} {c2} {c3} {c4}"
println "d -> ",d()
println

