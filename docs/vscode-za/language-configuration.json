{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"", "notIn": ["string"] },
    { "open": "`", "close": "`", "notIn": ["string"] }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["`", "`"]
  ],
  "indentationRules": {
    "increaseIndentPattern": "\\b(for|while|if|case|struct|enum|def|try|test|foreach)\\b",
    "decreaseIndentPattern": "\\b(endfor|endwhile|endif|endcase|endstruct|endenum|end|endtry|endtest)\\b"
  }
} 