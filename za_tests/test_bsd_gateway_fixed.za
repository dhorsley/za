# Test BSD gateway functions after fix
# This test verifies that all gateway functions work correctly

println "Testing BSD gateway functions..."

# Test gw_interface()
println "Testing gw_interface():"
try
    interface = gw_interface()
    println "✓ gw_interface() returned: " + interface
catch err
    println "✗ gw_interface() failed: " + err
endtry

# Test gw_address()
println "Testing gw_address():"
try
    gateway = gw_address()
    println "✓ gw_address() returned: " + gateway
catch err
    println "✗ gw_address() failed: " + err
endtry

# Test gw_info()
println "Testing gw_info():"
try
    info = gw_info()
    println "✓ gw_info() returned: " + as_string(info)
    if info["interface"] != ""
        println "  Interface: " + info["interface"]
    else
        println "  Interface: (empty)"
    endif
    if info["gateway"] != ""
        println "  Gateway: " + info["gateway"]
    else
        println "  Gateway: (empty)"
    endif
catch err
    println "✗ gw_info() failed: " + err
endtry

println "Gateway function test completed."


