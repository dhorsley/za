# Test simple condition evaluation
println "=== Simple Condition Test ==="

# Test direct evaluation
result = 4 > 3
println "4 > 3:", result

# Test string replacement
test_str = "#>3"
println "Original:", test_str

# Test manual replacement with string literal
manual = "4 > 3"
println "Manual:", manual

# Test eval of manual
result = eval(manual)
println "Eval manual:", result

# Test string replacement function
test_str2 = "#>3"
test_str2 = test_str2.Replace("#", "4")
println "After replace:", test_str2

# Test eval of replaced string
result = eval(test_str2)
println "Eval replaced:", result