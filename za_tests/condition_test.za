# Test condition string processing
println "=== Condition String Processing Test ==="

# Test data
arr = [1, 2, 3, 4, 5]
println "Array:", arr

# Test condition string with quotes
condition = "#>3"
println "Condition:", condition

# Test element access and replacement
element = 4
println "Element:", element
println "Element > 3:", element > 3

# Test string replacement manually
test_str = "#>3"
println "Before replacement:", test_str
test_str = test_str.Replace("#", "4")
println "After replacement:", test_str

# Test evaluation
result = eval(test_str)
println "Eval result:", result

# Test find with condition string
println "find(arr, '#>3'):"
result = find(arr, '#>3')
println result