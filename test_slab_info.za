// Test script to check if mem_info() populates the Slab map
mem = mem_info()
echo "Memory info retrieved"
echo "Slab map keys: " + keys(mem.Slab)
echo "Slab map size: " + len(mem.Slab)

if len(mem.Slab) > 0 {
    echo "First slab entry:"
    first_key = keys(mem.Slab)[0]
    first_slab = mem.Slab[first_key]
    echo "  Name: " + first_key
    echo "  ActiveObjs: " + first_slab.ActiveObjs
    echo "  NumObjs: " + first_slab.NumObjs
    echo "  ObjSize: " + first_slab.ObjSize
} else {
    echo "No slab information found"
} 